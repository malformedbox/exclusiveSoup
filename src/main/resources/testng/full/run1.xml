<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Run Suite" data-provider-thread-count="1">
    <test name="Run Tests" parallel="methods" thread-count="5">
        <classes>
            <class name="tests.AlwaysTests">
                <methods>
                    <include name="alwaysPass"/>
                    <include name="alwaysFail"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>